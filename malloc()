#include <stdio.h>
#include <stdlib.h>

int main() {
    int *p;

    // allocate memory for 3 integers
    p = (int *)malloc(3 * sizeof(int));

    // store values
    p[0] = 10;
    p[1] = 20;
    p[2] = 30;

    // print values
    printf("Values: %d %d %d", p[0], p[1], p[2]);

    // free memory
    free(p);

    return 0;
}
